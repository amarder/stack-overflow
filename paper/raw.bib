	pages = {77--131},
	file = {Kagdi et al. - 2007 - A survey and taxonomy of approaches for mining sof.pdf:/Users/amarder/Library/Application Support/Zotero/Profiles/qh86tkcr.default/zotero/storage/6E7H9D3C/Kagdi et al. - 2007 - A survey and taxonomy of approaches for mining sof.pdf:application/pdf;Snapshot:/Users/amarder/Library/Application Support/Zotero/Profiles/qh86tkcr.default/zotero/storage/M9CZMBJB/abstract\;jsessionid=E0C5638685B6DB2811E6BF0DCFBB343E.html:text/html}
