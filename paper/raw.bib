	year = {2012},
	keywords = {C13, C23, Econometrics, Economic Theory, I10, Low birthweight, Panel data, quantile regression, Statistics for Business/Economics/Mathematical Finance/Insurance, unobserved heterogeneity},
	pages = {1593--1633},
	file = {art%3A10.1007%2Fs00181-012-0570-8.pdf:/Users/amarder/Library/Application Support/Zotero/Profiles/qh86tkcr.default/zotero/storage/E2SCWUAE/art%3A10.1007%2Fs00181-012-0570-8.pdf:application/pdf;Snapshot:/Users/amarder/Library/Application Support/Zotero/Profiles/qh86tkcr.default/zotero/storage/GRPUSWFS/s00181-012-0570-8.html:text/html}
